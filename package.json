{
  "name": "react-base-ui",
  "version": "0.0.0-alfa.1",
  "author": "Cruz Bonilla Alejandro",
  "description": "React Base UI Components",
  "license": "MIT",
  "module": "dist/react-base-ui.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/AlejandroCruzBonilla/react-base-ui",
  "repository": {
    "url": "https://github.com/AlejandroCruzBonilla/react-base-ui",
    "type": "git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "keywords": [
    "react",
    "typescript",
    "components",
    "base",
    "ui",
    "alex139139"
  ],
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "styles:build": "sass src/styles/index.scss dist/styles/react-base-ui.css --style compressed --no-source-map",
    "styles:watch": "sass --watch src/styles/index.scss .storybook/styles/react-base-ui.css --no-source-map",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "size-limit": [
    {
      "path": "dist/react-base-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-base-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@size-limit/preset-small-lib": "^11.1.2",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-onboarding": "^8.0.9",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-webpack5": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@webpack-cli/generators": "^3.0.7",
    "autoprefixer": "^10.4.19",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "size-limit": "^11.1.2",
    "storybook": "^8.0.9",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "typescript-declaration-webpack-plugin": "^0.3.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
